<header>TinyApp Application</header>

<form method="POST" action="/logout">
	<% if(user){ %>
		<%= user.email %>
		<form action="/logout" method="POST">
		<input type="submit" value="Logout">
	</form>
	 <% } else { %>
		<a href="/register">Register Here</a>
		<a href="/login">Login Here</a>		
     <% } %>
</form>
